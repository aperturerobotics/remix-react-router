{
  "private": true,
  "workspaces": {
    "packages": [
      "packages/react-router",
      "packages/react-router-dom"
    ]
  },
  "scripts": {
    "build": "cd ./packages/react-router && npm run build && cd ../react-router-dom && npm run build",
