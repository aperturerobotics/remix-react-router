{
  "name": "@aptre/remix-react-router-dom",
  "version": "1.2.3",
  "description": "Declarative routing for React web applications",
  "keywords": [
    "react",
    "router",
    "route",
    "routing",
    "history",
    "link"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aperturerobotics/remix-react-router.git",
    "directory": "packages/react-router-dom"
  },
  "license": "MIT",
  "author": "Remix Software <hello@remix.run>",
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@aptre/remix-react-router": "1.2.3",
    "@remix-run/router": "1.20.0"
  },
  "devDependencies": {
    "esbuild": "^0.24.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "files": [
    "dist/",
    "LICENSE.md",
    "README.md",
    "server.d.ts",
    "server.js",
    "server.mjs"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:types && npm run build:lib",
    "build:lib": "npm run build:lib:client && npm run build:lib:server",
    "build:lib:client": "npm run build:esbuild -- --platform=browser --outfile=./dist/index.mjs ./index.tsx",
    "build:lib:server": "npm run build:esbuild -- --platform=node --outfile=./dist/server.mjs ./server.tsx",
    "build:esbuild": "esbuild --bundle --external:react --external:react-dom --external:@aptre/remix-react-router --external:remix-run/router --format=esm --define:__DEV__=false --tree-shaking=true --target=es2022",
    "build:types": "tsc --project tsconfig.json --noEmit false --emitDeclarationOnly --declaration --outDir ./dist/"
  }
}
